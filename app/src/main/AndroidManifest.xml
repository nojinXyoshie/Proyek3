<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.nojinx2.lelang">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
            android:name=".app.MyApplication"
            android:allowBackup="true"
            android:largeHeap="true"
            android:icon="@drawable/logo"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".auth.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.example.nojinx2.lelang.HomeActivity"/>
        <activity android:name="com.example.nojinx2.lelang.auth.Register" android:theme="@style/AppTheme"/>
        <activity android:name="com.example.nojinx2.lelang.Detail_paket"/>
        <activity android:name="com.example.nojinx2.lelang.PenjualActivity"/>
    </application>

</manifest>